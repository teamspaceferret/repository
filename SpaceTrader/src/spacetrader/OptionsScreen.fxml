<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.input.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spacetrader.OptionsScreenController">
   <children>
      <Label layoutX="14.0" layoutY="23.0" prefHeight="53.0" prefWidth="99.0" text="Options">
         <font>
            <Font size="24.0" />
         </font></Label>
      <SplitMenuButton layoutX="14.0" layoutY="85.0" mnemonicParsing="false" text="Sound Options">
        <items>
          <MenuItem fx:id="toggleBGButton" mnemonicParsing="false" onAction="#toggleBackgroundMusic" text="Mute Background Music" />
          <MenuItem fx:id="toggleSEButton" mnemonicParsing="false" onAction="#toggleSoundEffects" text="Mute Sound Effects" />
            <MenuItem fx:id="backShortcut" mnemonicParsing="false" onAction="#backAction" text="Unspecified Action" visible="false">
               <accelerator>
                  <KeyCodeCombination alt="UP" code="ESCAPE" control="UP" meta="UP" shift="ANY" shortcut="UP" />
               </accelerator>
            </MenuItem>
        </items>
      </SplitMenuButton>
      <Button layoutX="14.0" layoutY="337.0" mnemonicParsing="false" onAction="#backAction" prefHeight="40.0" prefWidth="147.0" text="Back (shift+esc)">
         <font>
            <Font size="17.0" />
         </font></Button>
   </children>
</AnchorPane>
